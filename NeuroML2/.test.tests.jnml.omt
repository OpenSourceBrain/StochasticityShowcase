# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: LEMS_Tests.xml 
engine: jNeuroML
mep: .test.tests.mep
experiments:
  spikeGenRegular:
    observables:
      spike times:
        spiketimes file: 
          path: regular50.spikes
          format: ID_TIME
          id: 0
        tolerance: 3.4e-6
  spikeGenRegular_rate:
    observables:
      spike rate:
        spiketimes file: 
          path: regular50.spikes
          format: ID_TIME
          id: 0
        tolerance: 3.4e-6

