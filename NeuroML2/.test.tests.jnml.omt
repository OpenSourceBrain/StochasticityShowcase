# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: LEMS_Tests.xml 
engine: jNeuroML
mep: .test.tests.mep
experiments:
    
  spikeGenRegular50:
    observables:
      spike times:
        spiketimes file: 
          path: regular.all.spikes
          format: ID_TIME
          id: 0
        tolerance: 1.3157894736928305e-05
        
